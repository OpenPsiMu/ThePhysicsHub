<DOCTYPE! html>
<head>
  <style>
    :root{
        --scaling_factor1: 75;
    }

    svg{
      margin: 1em;
      width: 25rem;
      height: 15rem;
      background-color: #151515;
      border-radius: .3rem;
    }
    .unit{
      stroke: white;
      stroke-width: .0005rem;
      transition-duration: .5s;
    }
    .square{
      fill: cornflowerblue;
    }
    .arrow{
      fill: white;
    }

    #first.square{
      transform: translate(50%, 50%) scale(var(--scaling_factor1), var(--scaling_factor1)) translate(0.5px, 0);
      transition-delay: 0.5s;
    }

    #first.spring{
      transform: translate(12.5%, 50%) scale(calc(var(--scaling_factor1)*2), var(--scaling_factor1)) translate(0.5px, 0);
      transition-delay: 0.5s;
    }

    #first.wall{
      transform: translate(12.5%, 50%) scale(calc(var(--scaling_factor1)*.2), calc(var(--scaling_factor1)*.3)) translate(-0.5px, 0);
      stroke-width: 0.003rem;
    }
    .latex{
      fill: white;
    }
    #first.arrow, #first.F{
      transform: translate(42.5%, 50%) scale(calc(1.2*var(--scaling_factor1)), calc(-.9*var(--scaling_factor1))) translate(0.5px, 0);
      opacity: 0;
      transition-delay: 0.5s;
      transition-duration: 0.5s;
    }
    #first.F{
      transform: translate(52.5%, 40%) scale(calc(0.3*var(--scaling_factor1)), calc(-.3*var(--scaling_factor1))) translate(0.5px, 0);
      stroke: black;
      transition-delay: 0.5s;
      transition-duration: 0.5s;
    }

    svg#first:hover>#first.spring{
      transform: translate(12.5%, 50%) scale(calc(var(--scaling_factor1)*1.07), var(--scaling_factor1)) translate(0.5px, 0);
      transition-delay: 0s;
    }
    svg#first:hover #first.square{
      transform: translate(32.5%, 50%) scale(var(--scaling_factor1), var(--scaling_factor1)) translate(0.5px, 0);
      transition-delay: 0s;
    }
    svg#first:hover #first.arrow, svg#first:hover #first.F{
      opacity: 1;
    }

    #vec.arrow{
      transform: translate(54.5%, 32%) scale(calc(0.2*var(--scaling_factor1)), calc(-.2*var(--scaling_factor1))) translate(0.5px, 0);
